{
  "configurations": [
    {
      "name": "ROS2 Humble + PX4",
      "includePath": [
        "${workspaceFolder}/**",
        "/opt/ros/humble/include/**",
        "/usr/include/**",
        "/usr/local/include/**",
        "${env:HOME}/px4_ws/**",
        "${env:HOME}/PX4-Autopilot/**"
      ],
      "defines": [
        "ROS2"
      ],
      "compilerPath": "/usr/bin/g++",
      "cStandard": "c17",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-x64",
      "browse": {
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db",
        "path": [
          "${workspaceFolder}/**",
          "/opt/ros/humble/include/**",
          "/usr/include/**",
          "/usr/local/include/**",
          "${env:HOME}/px4_ws/**",
          "${env:HOME}/PX4-Autopilot/**"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
