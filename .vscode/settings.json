{
    "python.analysis.extraPaths": [
        "${workspaceFolder}/../../devel/lib/python3/dist-packages",
        "/opt/ros/humble/lib/python3/dist-packages",
        "${workspaceFolder}/src",
        "${workspaceFolder}/nodes",
        "${workspaceFolder}/scripts"
    ],
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/../../devel/lib/python3/dist-packages",
        "/opt/ros/humble/lib/python3/dist-packages",
        "${workspaceFolder}/src",
        "${workspaceFolder}/nodes",
        "${workspaceFolder}/scripts"
    ],
    "python.analysis.exclude": [
        "**/build",
        "**/devel",
        "**/install",
        "**/extern",
        "**/src/**/CMakeLists.txt",
        "**/__pycache__",
        "**/*.pyc"
    ],
    "idf.pythonInstallPath": "/bin/python3",
    "files.associations": {
        "CMakeLists.txt": "cmake",
        "*.cmake": "cmake"
    },
    "search.exclude": {
        "extern/**": true
    },
    "cmake.configureOnOpen": false
}
